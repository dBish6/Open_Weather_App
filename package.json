{
  "name": "open_weather_app",
  "version": "1.0.0",
  "description": "A semi simple weather app that uses postcss and via open weather api.",
  "private": true,
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "axios": "^1.3.2",
    "cross-env": "^7.0.3",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "cssnano": "^5.1.14",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-preset-env": "^8.0.1",
    "postcss-pxtorem": "^6.0.0"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "build:css": "postcss src/styles/*.css --dir src/styles/build --config src/utils/postcss.config.js",
    "watch:css": "postcss src/styles/*.css --dir src/styles/build --watch --config src/utils/utils/postcss.config.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
